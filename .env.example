# Environment variables for JWT MySQL Automation Service
# Copy this file to .env and update with your actual values

# MySQL Configuration
MYSQL_HOST=mysql
MYSQL_USER=root
MYSQL_PASS=your_secure_password_here
MYSQL_DB=arkane_settings

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here

# Database User (optional - for non-root user)
MYSQL_USER_NAME=arkane_user
MYSQL_USER_PASS=your_user_password_here
